module program_definition: 
    snakefile: "program_definition.smk"
use rule * from program_definition as program_definition_*

module immune_evasion: 
    snakefile: "immune_evasion.smk"
use rule * from immune_evasion as immune_evasion_*

module proliferation: 
    snakefile: "proliferation.smk"
use rule * from proliferation as proliferation_*

module tumorigenesis: 
    snakefile: "tumorigenesis.smk"
use rule * from tumorigenesis as tumorigenesis_*

rule all:
    input:
        rules.program_definition_all.input,
        rules.immune_evasion_all.input
        rules.proliferation_all.input
        rules.tumorigenesis_all.input
    default_target: True